{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "type-check": {
      "dependsOn": []
    },
    "lint": {
      "dependsOn": []
    },
    "lint:fix": {
      "cache": false
    },
    "prettier": {},
    "prettier:fix": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {}
  }
}